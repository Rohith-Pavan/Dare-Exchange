{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "DJANGO_SETTINGS_MODULE=dareproject.production_settings python manage.py migrate && DJANGO_SETTINGS_MODULE=dareproject.production_settings gunicorn dareproject.wsgi:application",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}